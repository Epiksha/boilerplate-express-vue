{
    "name": "@epiksha/boilerplate-express-vue",
    "version": "1.0.0",
    "description": "",
    "main": "server.js",
    "scripts": {
        "build": "webpack --config webpack.production.js",
        "lint": "eslint \"*/**/*.{js,ts}\" --max-warnings 0",
        "postinstall": "husky install",
        "test": "cross-env NODE_ENV=test jest --runInBand --maxWorks=1 --coverage",
        "test:dev": "cross-env NODE_ENV=test jest --watch --verbose=true --coverage",
        "test:unit": "cross-env NODE_ENV=test jest unit",
        "test:integration": "cross-env NODE_ENV=test jest integration",
        "server": "cross-env NODE_ENV=production nodemon server.js",
        "server:dev": "cross-env NODE_ENV=development nodemon server.js",
        "start": "concurrently \"npm run build\" \"npm run server\"",
        "start:dev": "concurrently \"npm run server:dev\"  \"npm run watch\"",
        "watch": "cross-env NODE_ENV=development webpack serve --config webpack.development.js"
    },
    "keywords": [],
    "author": "Calvin Tiley",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "7.6.4",
